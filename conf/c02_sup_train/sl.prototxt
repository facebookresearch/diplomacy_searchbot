    skip_validation: false;
    learnable_A: false;
    learnable_alignments: false;
    avg_embedding: false;
    write_jsonl: true;
    value_loss_weight: 0.3;
    value_decoder_init_scale: 0.01;
    value_decoder_clip_grad_norm: 1e-7;
    value_decay_alpha: 0.9;
    min_rating_percentile: 0.5;
    value_loss_weight: 0.9;
    batch_size: 1000;
}
